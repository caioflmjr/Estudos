void IntercalaVetores(char *V1, char *V2, char *V3)
{
    if (*V1 == '\0' && *V2 == '\0')
    {
        *V3 = '\0';
        return;
    }

    if (*V1 != '\0')
    {
        *V3 = *V1;
        V3++;
        V1++;
    }
    if (*V2 != '\0')
    {
        *V3 = *V2;
        V3++;
        V2++;
    }
    IntercalaVetores(V1, V2, V3);
}